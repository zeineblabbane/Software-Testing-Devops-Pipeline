{
    "family": "zeineb-tsk-def-user-management-app-pipeline",
    "containerDefinitions": [
        {
            "name": "user-management-app",
            "image": "zeineblabbane/user-management",
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 5000,
                    "protocol": "tcp"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "memory": "1024",
    "cpu": "512"
}